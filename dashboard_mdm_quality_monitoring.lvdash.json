{
  "datasets": [
    {
      "name": "ds_security_master_summary",
      "displayName": "Security Master Summary (KPIs)",
      "queryLines": [
        "SELECT total_securities, golden_records_created_last_30d FROM demo_generator.saswata_sengupta_agenticmdm.gold_security_master_summary "
      ]
    },
    {
      "name": "ds_open_identifier_conflicts",
      "displayName": "Open Identifier Conflicts (Latest KPI)",
      "queryLines": [
        "SELECT open_identifier_conflicts_latest FROM demo_generator.saswata_sengupta_agenticmdm.gold_open_identifier_conflicts_latest "
      ]
    },
    {
      "name": "ds_exposure_impact_today_emea",
      "displayName": "Exposure Impact (EMEA Today KPI)",
      "queryLines": [
        "SELECT CAST(MAX(`date`) AS DATE) AS latest_date, 'EMEA' AS region, SUM(CASE WHEN `region`='EMEA' AND `date` = (SELECT MAX(`date`) FROM demo_generator.saswata_sengupta_agenticmdm.gold_exposure_impact) THEN `misclassified_exposure_usd` ELSE 0 END) AS emea_misclassified_exposure_usd_today FROM demo_generator.saswata_sengupta_agenticmdm.gold_exposure_impact "
      ]
    },
    {
      "name": "ds_quality_daily",
      "displayName": "Master Quality Timeseries (Daily)",
      "queryLines": [
        "SELECT date, region, sub_product, identifier_conflict_events, duplicate_candidates, adr_etf_mapping_issues FROM demo_generator.saswata_sengupta_agenticmdm.gold_master_quality_timeseries ORDER BY date, region, sub_product "
      ]
    },
    {
      "name": "ds_quality_weekly",
      "displayName": "Master Quality Weekly Rollup",
      "queryLines": [
        "SELECT week_start, region, sub_product, weekly_identifier_conflicts, weekly_duplicate_candidates, weekly_mapping_issues, COALESCE(weekly_duplicate_candidates,0)+COALESCE(weekly_mapping_issues,0) AS total_weekly_issues FROM demo_generator.saswata_sengupta_agenticmdm.gold_master_quality_weekly ORDER BY week_start, region, sub_product "
      ]
    },
    {
      "name": "ds_vendor_change_log",
      "displayName": "Vendor Change Log",
      "queryLines": [
        "SELECT change_date, vendor, change_type, scope_region, module, notes, error_codes FROM demo_generator.saswata_sengupta_agenticmdm.gold_vendor_change_log ORDER BY change_date "
      ]
    },
    {
      "name": "ds_lineage_completeness",
      "displayName": "Lineage Completeness Timeseries",
      "queryLines": [
        "SELECT date, region, sub_product, lineage_completeness_pct FROM demo_generator.saswata_sengupta_agenticmdm.gold_lineage_completeness_timeseries ORDER BY date, region, sub_product "
      ]
    },
    {
      "name": "ds_exposure_impact_cumulative",
      "displayName": "Exposure Impact Cumulative",
      "queryLines": [
        "SELECT date, region, sub_product, running_total_misclassified_usd FROM demo_generator.saswata_sengupta_agenticmdm.gold_exposure_impact_cumulative ORDER BY date, region, sub_product "
      ]
    },
    {
      "name": "ds_holdings_sector",
      "displayName": "Holdings Sector (Filter Source)",
      "queryLines": [
        "SELECT date, region, sub_product, sector_code FROM demo_generator.saswata_sengupta_agenticmdm.silver_holdings_report ORDER BY date, region, sub_product, sector_code "
      ]
    }
  ],
